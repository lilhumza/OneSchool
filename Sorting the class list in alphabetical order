import java.util.Scanner;
public class classList {
	public static void main(String[] args) 
    {
        int n;
        String temp;
        Scanner s = new Scanner(System.in);
        System.out.print("Enter number of students in the class:");
        n = s.nextInt();
        String names[] = new String[n];
        Scanner s1 = new Scanner(System.in);
        System.out.println("Enter all the names:");
	//Takes all of the names that are inputted and re-writes it in alphabetical order
        for(int i = 0; i < n; i++)
        {
            names[i] = s1.nextLine();
        }
        for (int i = 0; i < n; i++) 
        {
            for (int j = i + 1; j < n; j++) 
            {
                if (names[i].compareTo(names[j])>0) 
                {
                    temp = names[i];
                    names[i] = names[j];
                    names[j] = temp;
                }
            }
        }
	//Displays the class list in alphabetical order
        System.out.print("Class list in alphabetical order:");
        for (int i = 0; i < n - 1; i++) 
        {
            System.out.println(names[i]);
        }
        System.out.print(names[n - 1]);
    }
	
}
